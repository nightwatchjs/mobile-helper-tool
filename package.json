    "@types/cli-progress": "^3.11.0",
    "@types/download": "^8.0.1",
    "@types/inquirer": "^8.2.3",
    "@types/minimist": "^1.2.2",
    "@types/mockery": "^1.4.30",
    "@types/node": "^18.7.21",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "mockery": "^2.1.0",
    "nightwatch": "^3.6.2",
